{
  "name": "IRC for Chrome",
  "description": "An IRC client for Chrome",
  "version": "0.2",
  "manifest_version": 2,
  "permissions": [
    "experimental",
    "windows",
    "storage",
    "tabs"
  ],
  "app": {
    "launch": {
      "local_path": "src/main.html"
    }
  },
  "icons": { "16": "icon/icon16.png", "48": "icon/icon48.png", "128": "icon/icon128.png" }
}
