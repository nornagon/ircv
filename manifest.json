{
  "name": "IRC",
  "description": "An IRC client for Chrome",
  "version": "1",
  "manifest_version": 2,
  "permissions": [
    "experimental",
    "windows",
    "storage",
    "tabs"
  ],
  "app": {
    "launch": {
      "local_path": "main.html"
    }
  },
  "icons": { "16": "icon16.png", "48": "icon48.png", "128": "icon128.png" }
}
